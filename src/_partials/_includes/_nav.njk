    {# ------------- menu ------------- #}
    <div id="navItems">
      {# ------------- Desktop Nav ------------- #}
      <div class="hidden lg:block">
        <div class="flex flex-row items-top">
          <ul class="flex flex-row pt-4 items-top">
            {%- for navItem in nav.main -%}
              {%- if navItem.showInMainNav -%}
                <li>
                  <a href="{{navItem.url}}">
                    <div class="relative ml-12 cursor-pointer hover-link" x-data="{tooltip : null}" {# @click="tooltip = !tooltip" #} x-on:mouseover="tooltip = true" x-on:mouseleave="tooltip = false">
                      <span class="text-xl ">{{navItem.name}}</span>
                      <p x-cloak x-show.transition.duration.50="tooltip" class="absolute right-0 w-56 pt-2 mt-8 text-base text-right transition-all -top-2">{{navItem.description |safe }}&nbsp;</p>
                    </div>
                  </a>
                </li>
              {%- endif -%}
            {%- endfor -%}
          </ul>
        </div>
      </div>
      {# ------------- Mobile Nav ------------- #}

      {# ------------- button ------------- #}
      <div class="relative flex flex-row items-baseline h-12 cursor-pointer lg:hidden">
        <div class="realtive " @click="animate=!animate" {# aria-expanded="animate" #}>
          <span class="sr-only">Menu</span>
          <svg x-show.transition.duration.150="!animate" class="absolute inset-y-0 right-0 w-8 h-8 text-u2-black" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor" aria-hidden="true">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h16M4 18h16"/>
          </svg>
          <svg x-show.transition.duration.150="animate" class="absolute inset-y-0 right-0 w-8 h-8 text-u2-black" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor" x-cloak="x-cloak">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12"/>
          </svg>
        </div>
      </div>

    </div>
  </div>
  {# ------------- pannel ------------- #}
  <div class="block overflow-hidden transition-all max-h-0" x-cloak x-ref="menuPanel" x-bind:style="animate ? 'max-height: ' + $refs.menuPanel.scrollHeight + 'px; opacity: 1' : 'max-height: 0; opacity: .6'" @keydown.escape.window="animate = false">

    <div class="w-full max-h-full pt-4 pb-1 rounded-lg">
      <ul class="">
        {%- for navItem in nav.main -%}
          {%- if navItem.showInMainNav -%}
            <li>
              <a href="{{navItem.url}}">
                <div class="p-4 mb-2 cursor-pointer bg-u2-black text-u2-white">
                  <p class="text-2xl">{{navItem.name}}</p>
                  <p class="mt-1 text-base text-u2-highlight-grey">{{navItem.description | safe}}</p>
                </div>
              </a>
            </li>
          {%- endif -%}
        {%- endfor -%}
      </ul>
    </div>
  </div>

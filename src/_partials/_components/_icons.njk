  {% if iconName == 'webinar' %}
    {{webinar(colour = colour, size = size)}}
  {% elif iconName == 'article' %}
    {{article(colour = colour, size = size)}}
  {% elif iconName == 'whitepaper' %}
    {{whitepaper(colour = colour or false, size = size)}}

  {% elif iconName == 'ebook' %}
    {{ebook(colour = colour , size = size)}}
  {% elif iconName == 'video series' %}
    {{videoSeries(colour = colour, size = size)}}
  {% endif %}
{% endmacro %}


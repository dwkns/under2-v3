{%- macro pageHeader(breadcrumb, title, subTitle, insightsSection, breadcrumbTag = "p", titleTag = "h1", underTitleContent="", rightMargin="") -%}
  {%- if breadcrumb.breadCrumbIsH1 -%}
    {%- set breadcrumbTag = "h1"  -%}
    {%- set titleTag = "p"  -%}
  {%- endif -%}
  <section class="mx-auto mt-16 max-w-screen-3xl px-u2-gutter-sm sm:px-u2-gutter">
    <div class="grid grid-cols-12 gap-u2-gap">
      {%- if breadcrumb.breadCrumbLink -%}
        <a class="flex items-center col-span-12 group text-u2-highlight-grey" href="{{breadcrumb.breadCrumbLink}}">
          {% import "_components/_icons.njk" as icons with context %}
          {{ icons.back()}}
          <{{breadcrumbTag}} class="my-0 ml-1 breadcrumb group-hover:hover-link-group group-hover:text-u2-black ">{{breadcrumb.title}}</{{breadcrumbTag}}>
        </a>
      {%- else -%}
        <{{breadcrumbTag}} class="col-span-12 my-0 breadcrumb">{{breadcrumb.title}}</{{breadcrumbTag}}>
      {%- endif -%}
      <div class="col-span-12 lg:col-span-6">
        <{{titleTag}} class="my-0 text-4xl font-extrabold tracking-tight sm:text-5xl {{rightMargin}}">{{title | safe }}</{{titleTag}}>
        {{underTitleContent | safe }}
      </div>
      <div class="col-span-12 mt-0 text-2xl font-semibold lg:col-span-6 sm:text-3xl {{'lg:col-start-8' if insightsSection }}">{{subTitle | safe}}</div>
    </div>
  </section>
{%- endmacro -%}

{%- macro blockquote(
      quote="",
      cite="",
      citeLink="",
      caption="",
      before="&ldquo;",
      after="&ldquo;",
      linkCite=false
      
      ) -%}
  {%- if linkCite -%}
    {%- set citeHTML -%}
    <cite>
      <a href="{{citeLink}}" class="link">{{cite}}</a>
    </cite>
    {%- endset -%}
  {%- else -%}
    {%- set citeHTML -%}
    <cite>{{cite}}</cite>
    {%- endset -%}
  {%- endif -%}

  <div class="my-8 lg:my-16 bg-u2-white text-u2-black">
    <figure class="flex flex-col justify-center w-full py-4 border-l-8 border-u2-gold bg-u2-light-grey">
      <blockquote cite="{{citeLink}}" >
        <p class="px-6 mt-0 text-xl font-semibold sm:w:9/12 sm:text-2xl ">{{before | safe }}{{quote}}{{after | safe}}</p>
      </blockquote>
      <figcaption class="pl-6 mt-2 text-base sm:w:9/12 sm:text-xl">{{caption}}
        {{citeHTML | safe}}
      </figcaption>
    </figure>
  </div>
{%- endmacro -%}

{% macro faq(faqs) %}
  <ul class="mx-0">
    {% for faq in faqs %}
      <li class="relative" x-data="{show:null}">

        <button type="button" class="w-full {{'mt-6 md:mt-8' if not loop.first else '' }} text-left focus:outline-none hover-link " @click="show=!show">
          <div class="flex items-start">
            <span class="inline-flex transition-transform ease-out transform translate-y-2 cursor-pointer duration-400" :class="{'rotate-90': show}" >
              <svg class="w-4 h-4 fill-currentXXX text-u2-highlight-greyXXX" viewbox="0 0 26 46" width="26" height="46" xmlns="http://www.w3.org/2000/svg">
                <path d="M3.914.852l20.628 20.627a2 2 0 010 2.829L3.914 44.935A2 2 0 01.5 43.521V2.266A2 2 0 013.914.852z"/></svg>
            </span>
            <span class="inline-flex ml-3 text-2xl font-display text-u2-highlight-greyXXX hover:text-u2-blackXXX">
              {{ faq.question }}
            </span>
          </div>
        </button>

        <div  x-cloak="x-cloak" class="relative w-11/12 pl-4 overflow-hidden transition-all duration-500 max-h-0 md:w-10/12" 
              x-ref="faqAnswer" 
              x-bind:style="show ? 'max-height: ' + $refs.faqAnswer.scrollHeight + 'px' : ''">
          {% for paragraph in faq.answer %}
            <p class="pl-3 mt-6 text-xl">{{ paragraph | safe}}</p>
          {% endfor %}
        </div>
      </li>
    {% endfor %}
  </ul>
{% endmacro %}

